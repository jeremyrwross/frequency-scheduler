{
    "name": "jeremyrwross/frequency-scheduler",
    "type": "library",
    "version": "1.0.0",
    "keywords": ["laravel", "scheduler", "frequency", "php", "cron"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jeremyrwross\\FrequencyScheduler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jeremyrwross\\FrequencyScheduler\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jeremy Ross",
            "email": "jeremyrwross@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jeremyrwross\\FrequencyScheduler\\FrequencySchedulerServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "require-dev": {
        "pestphp/pest": "^3.5",
        "laravel/pint": "^1.18",
        "nunomaduro/collision": "^8.5",
        "orchestra/testbench": "^9.6",
        "pestphp/pest-plugin": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
